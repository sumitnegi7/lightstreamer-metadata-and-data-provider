<?xml version="1.0"?>
<adapters_conf id="IQO_NODE">
        <metadata_adapter_initialised_first>N</metadata_adapter_initialised_first>
         <metadata_provider>
                <adapter_class>ROBUST_PROXY_FOR_REMOTE_ADAPTER</adapter_class>
                <classloader>log-enabled</classloader>
                <param name="auth">Y</param>
                <param name="auth.credentials.1.user">username</param>
                <param name="auth.credentials.1.password">password</param>
                <param name="request_reply_port">4002</param>
                <param name="timeout">36000000</param>
        </metadata_provider>
        <data_provider>
                <adapter_class>ROBUST_PROXY_FOR_REMOTE_ADAPTER</adapter_class>
                <classloader>log-enabled</classloader>
                <param name="auth">Y</param>
                <param name="auth.credentials.1.user">username</param>
                <param name="auth.credentials.1.password">password</param>
                <param name="request_reply_port">4003</param>
                <param name="notify_port">4001</param>
                <param name="timeout">36000000</param>
        </data_provider>
</adapters_conf>
